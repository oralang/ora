// simple_string.ora
// Demonstrates Ora's simplified string handling for smart contracts

contract StringDemo {
    // Basic string usage
    fn stringExample() -> string {
        let simple : string = "This is a simple ASCII string";
        
        return simple;
    }
    
    // Using strings with other types
    fn stringWithTypes() -> string {
        var str_value: string = "Example string";
        return str_value;
    }
}
