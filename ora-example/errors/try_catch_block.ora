contract TryCatchBlock {
    error Fail;

    log Success();
    log Failed();

    fn mayFail(x: u256) -> !u256 {
        if (x == 0) {
            return error.Fail;
        }
        return x + 1;
    }

    fn run(x: u256) {
        try {
            let y = mayFail(x);
            log Success();
        } catch (e) {
            log Failed();
        }
    }
}
