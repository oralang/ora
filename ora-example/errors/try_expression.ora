contract TryExpr {
    
    error Fail;

    pub fn init() {}

    fn test(x: u256) -> u256 {
        return x * 2;
    }

    fn mayFail(x: u256) -> !u256
        requires(x < std.constants.U256_MAX)
    {
        if (x == 0) {
            return error.Fail;
        }
        return x + 1;
    }

    fn run(x: u256) -> !u256 {
        let y: u256 = try mayFail(x);
        return y;
    }

    pub fn main(x: u256) {
        try {
            run(x);
        } catch {
        }
    }
}
