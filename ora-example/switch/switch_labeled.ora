contract LabeledSwitchDemo {
    fn init() {
        var x: u256 = 7;

        // Labeled switch with labeled block arm and labeled-continue operand
        outer: switch (x) {
            0 => { x = 100; },
            1...5 => arm_block: { x = x + 1; },
            else => {
                continue :outer (0);
            }
        }

        _ = x;
    }
}
