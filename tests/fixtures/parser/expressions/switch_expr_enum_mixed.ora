contract SwitchExprEnumMixed {
    enum Color: u8 { Red, Green, Blue, Indigo }
    pub fn demo() {
        let c: Color = Color.Green;
        let v: u256 = switch (c) {
            Color.Red => 1,
            Green => 2,
            Blue...Indigo => 3,
            else => 9,
        };
        _ = v;
    }
}
