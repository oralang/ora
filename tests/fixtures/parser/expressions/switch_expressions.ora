contract SwitchExprDemo {
    pub fn f(x: u256) -> u256 {
        // Switch expression with default as expression
        let y = switch (x) { else => 42 };
        return y;
    }

    pub fn g(x: u256) -> u256 {
        // Switch expression with pattern arm and default expression
        let y = switch (x) { 1 => 10, else => x + 1 };
        return y;
    }

    // Negative: switch expression arm must not be a block (commented out)
    // pub fn bad(x: u256) -> u256 {
    //     let y = switch (x) { 1 => { 42 }, else => 0 };
    //     return y;
    // }
}


