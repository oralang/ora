contract SwitchStmtDemo {
    pub fn f(x: u256) {
        // Switch statement with expression default (allowed; wrapped to block)
        switch (x) {
            1 => { log Event1(); },
            else => 0;
        }
    }

    pub fn g(x: u256) {
        // Switch statement with block default
        switch (x) {
            1 => 2;
            else => { log Event2(); }
        }
    }
}


