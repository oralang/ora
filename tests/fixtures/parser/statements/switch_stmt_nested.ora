contract SwitchStmtNested {
    pub fn demo() {
        var x: u256 = 1;
        switch (x) {
            0 => x = 10;
            1 => {
                switch (x + 1) {
                    2 => x = 20;
                    else => x = 21;
                }
            };
            else => x = 99;
        }
        _ = x;
    }
}
