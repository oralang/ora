contract SwitchStmtLabeledMulti {
    pub fn demo() {
        var x: u256 = 3;
        outer: switch (x) {
            0 => x = 0;
            1 => { continue :outer (2); };
            2 => { continue :outer (3); };
            3...4 => lbl: { x = x + 10; };
            else => { x = 99; }
        }
        _ = x;
    }
}
